					],
					"script": "source(output(\n\t\tColumn_1 as string\n\t),\n\tallowSchemaDrift: true,\n\tvalidateSchema: false,\n\tignoreNoFilesFound: false) ~> source1\nsource1 derive(EmpId = substring(Column_1, 1, 4),\n\t\tEmpName = substring(Column_1, 5, 10),\n\t\tDeptName = substring(Column_1, 15, 2),\n\t\tSalary = substring(Column_1, 17, 5),\n\t\tMobNo = substring(Column_1, 22, 10)) ~> DerivedColumn1\nDerivedColumn1 select(mapColumn(\n\t\tEmpId,\n\t\tEmpName,\n\t\tDeptName,\n\t\tSalary,\n\t\tMobNo\n\t),\n\tskipDuplicateMapInputs: true,\n\tskipDuplicateMapOutputs: true) ~> Select1\nSelect1 sink(allowSchemaDrift: true,\n\tvalidateSchema: false,\n\tinput(\n\t\tEmpNo as integer,\n\t\tEmpName as string,\n\t\tDeptName as string,\n\t\tsal as decimal(19,4),\n\t\tMobNo as long\n\t),\n\tdeletable:false,\n\tinsertable:true,\n\tupdateable:false,\n\tupsertable:false,\n\tformat: 'table',\n\tskipDuplicateMapInputs: true,\n\tskipDuplicateMapOutputs: true,\n\terrorHandlingOption: 'stopOnFirstError',\n\tmapColumn(\n\t\tEmpNo = EmpId,\n\t\tEmpName,\n\t\tDeptName,\n\t\tsal = Salary,\n\t\tMobNo\n\t)) ~> sink1"
				}
			},
