								"referenceName": "empfilter",
								"type": "DatasetReference"
							},
							"name": "sink2"
						}
					],
					"transformations": [
						{
							"name": "ConditionalSplit1"
						},
						{
							"name": "Sort1"
						},
						{
							"name": "Filter1"
						},
						{
							"name": "Sort2"
						}
					],
					"script": "source(output(\n\t\tempno as integer,\n\t\tename as string,\n\t\tjob as string,\n\t\tsal as decimal(19,4),\n\t\tcomm as decimal(19,4),\n\t\tdeptno as integer,\n\t\thiredate as date,\n\t\tmgr as integer,\n\t\ttotal as decimal(19,4)\n\t),\n\tallowSchemaDrift: true,\n\tvalidateSchema: false,\n\tisolationLevel: 'READ_UNCOMMITTED',\n\tformat: 'table') ~> source1\nsource1 split(deptno==10,\n\tdisjoint: true) ~> ConditionalSplit1@(deptno10, RestAllDepts)\nConditionalSplit1@deptno10 sort(asc(empno, false),\n\tcaseInsensitive: true) ~> Sort1\nConditionalSplit1@RestAllDepts filter(deptno!=40) ~> Filter1\nFilter1 sort(asc(empno, false)) ~> Sort2\nSort2 sink(allowSchemaDrift: true,\n\tvalidateSchema: false,\n\tumask: 0022,\n\tskipDuplicateMapInputs: true,\n\tskipDuplicateMapOutputs: true) ~> sink1\nSort1 sink(allowSchemaDrift: true,\n\tvalidateSchema: false,\n\tumask: 0022,\n\tskipDuplicateMapInputs: true,\n\tskipDuplicateMapOutputs: true) ~> sink2"
				}
			},
			"dependsOn": []
		},
